{
  "name": "hook0",
  "version": "1.0.0",
  "description": "Hook0 Frontend - Webhook As a Service",
  "contributors": [
    "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
    "David Sferruzza <david.sferruzza@gmail.com>"
  ],
  "private": true,
  "license": "SSPL-1.0",
  "files": [
    "src/**",
    "public/**",
    "README.md",
    "LICENSE.txt",
    ".browserslistrc",
    ".eslintrc.js",
    "tsconfig.json",
    "vue.config.js",
    "tailwind.config.js",
    "postcss.config.js",
    "babel.config.js",
    ".prettierrc.js"
  ],
  "scripts": {
    "//start:dev": "don't forget to first start the Rust API in order to then access to this frontend through the API http server",
    "serve": "vue-cli-service serve --mode dev --watch",
    "build": "vite build",
    "dev": "vite",
    "check": "tsc --noEmit && vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix ./src",
    "test": "jest --watchAll=false --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=25%",
    "test:ci": "jest --runInBand --coverage --collectCoverage",
    "format": "prettier --write src/ ./*.{js,ts,json}",
    "generate:types": "openapi-typescript http://localhost:8080/api/v1/swagger.json --output src/types.ts",
    "generate:types:prod": "openapi-typescript https://app.hook0.com/api/v1/swagger.json --output src/types.ts"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^30.0.2",
    "@ag-grid-community/core": "^30.0.2",
    "@ag-grid-community/styles": "^30.0.2",
    "@ag-grid-community/vue3": "^30.0.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.2.2",
    "codemirror": "^6.0.1",
    "date-fns": "^2.29.3",
    "fp-ts": "^2.11.6",
    "keycloak-js": "^22.0.0",
    "lodash.debounce": "^4.0.8",
    "ramda": "^0.29.0",
    "swagger-ui": "^4.15.0",
    "uuidv4": "^6.2.7",
    "vue": "^3.0.0",
    "vue-click-outside-element": "^3.1.2",
    "vue-codemirror": "^6.1.1",
    "vue-form-json-schema": "^3.0.0-alpha.0",
    "vue-promised": "^2.0.2",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@types/jest": "^29.2.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/ramda": "^0.29.1",
    "@types/swagger-ui": "^3.47.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.7.0",
    "jest": "^29.6.1",
    "openapi-typescript": "^6.0.2",
    "postcss": "^8.4.4",
    "prettier": "^2.8.8",
    "sass": "^1.32.6",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.4",
    "vite-plugin-checker": "^0.6.1",
    "vue-tsc": "^1.8.5"
  },
  "keywords": [
    "webhook",
    "webhooks",
    "web-hook",
    "connectors",
    "api",
    "webhook api",
    "webhook server"
  ],
  "volta": {
    "node": "16.13.1"
  }
}
